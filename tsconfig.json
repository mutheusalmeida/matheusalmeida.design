{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "types": ["@astrojs/image/client"],
    "typeRoots": [
      "./node_modules/@types",
      "./src/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
